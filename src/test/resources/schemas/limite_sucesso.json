{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "idLmt": {
      "type": "integer"
    },
    "ltConsumo": {
      "type": "integer"
    },
    "hrInicio": {
      "type": "string"
    },
    "hrFim": {
      "type": "string"
    },
    "equipamento": {
      "type": "object",
      "properties": {
        "idEquip": {
          "type": "integer"
        },
        "nmEquipamento": {
          "type": "string"
        },
        "tipo": {
          "type": "string"
        },
        "setor": {
          "type": "object",
          "properties": {
            "idSet": {
              "type": "integer"
            },
            "nmSetor": {
              "type": "string"
            },
            "nrAndar": {
              "type": "integer"
            }
          },
          "required": ["idSet", "nmSetor", "nrAndar"]
        }
      },
      "required": ["idEquip", "nmEquipamento", "tipo", "setor"]
    }
  },
  "required": ["idLmt", "ltConsumo", "hrInicio", "hrFim", "equipamento"]
}